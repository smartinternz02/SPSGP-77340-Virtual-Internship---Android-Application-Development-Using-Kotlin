<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <TextView
        android:id="@+id/title_label"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/common.padding"
        android:layout_marginEnd="@dimen/common.margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/action_label"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Venue title" />

    <TextView
        android:id="@+id/action_label"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/common.padding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/primary"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:text="@string/nearby.highlight.button_label" />

</merge>